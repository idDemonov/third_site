									a.submenu__link(href='index.html') Home
								li.submenu__item
									a.submenu__link(href='about.html') Result
								li.submenu__item
									a.submenu__link(href='log.html') Log In
								li.submenu__item
									a.submenu__link(href='register.html') Register
								li.submenu__item
									a.submenu__link(href='room.html') Room
						li.navigation-header__item
							a.navigation-header__link(href='header-footer.html') Вакансии
						li.navigation-header__item
							a.navigation-header__link(href='cards.html') Новости
						li.navigation-header__item.dropdown-menu Соглашения
							ul.submenu.submenu--documentation
								li.submenu__item
									a.submenu__link(href='header-footer.html') Footer-Header
