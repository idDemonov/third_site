mixin date-filter(value='19 авг - 23 авг')
	.date-filter
		button.date-filter__button(type='button')
				img(src= require('../../assets/img/button/dropdown.svg').default, alt='')
		input.date-filter__input(type='text' value="19 авг - 23 авг", pattern='[0-9]{2}\\s[а-я]{1,3}\\s-\\s[0-9]{2}\\s[а-я]{1,3}')